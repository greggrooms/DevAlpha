{
    "extends": "tslint-config-airbnb",
    "rules": {
      "import-name": false,
      "semicolon": [true, "never"],
      "trailing-comma": [true, "never"],
      "typedef": true,
      "object-shorthand-properties-first": false,
      "no-parameter-reassignment": false,
      "max-line-length": [true, 120],
      "no-boolean-literal-compare": false
    }
}
